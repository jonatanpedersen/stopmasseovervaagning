extend _base

block append head
	link(href="politikere.css",rel="stylesheet")

block main
	.section.header
		.container
			h1 Politikere
			p.lead Oversigt over Danske folketingspolitikere.
	.section
		.container
			.row
				each politican in politicians
					.col-xs-6.col-sm-4.col-lg-3
						a(href="/politikere/" + politican.id).card
							img(src="/img/" + politican.id + ".jpg").card-img-top.img-fluid
