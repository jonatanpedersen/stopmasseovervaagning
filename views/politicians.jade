extend _base

block append head
	link(href="politikere.css",rel="stylesheet")

block main
	.section.header
		.container
			h1 Politikere
			p.lead Oversigt over Danske folketingspolitikere.
	.section
		.container
			.row
				each politician in politicians
					.col-xs-6.col-sm-4.col-lg-3
						a(href="/politikere/" + politician.id).card
							img(src="/img/" + politician.id + ".jpg").card-img-top.img-fluid
							.card-block
								p.card-text= politician.name
